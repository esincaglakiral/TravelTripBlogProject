@using TravelTripProje.Models.Siniflar
@model List<Blog>


<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {


            <div class="bot-img">
                <a href="/Blog/BlogDetay/@item.ID"><img src="@item.BlogImage" alt="" style="height:100px;" /></a>
            </div>
            <div class="bot-desc" style="padding-bottom:20px;">
                <p>@item.Aciklama.Substring(0, 130) ...</p>
            </div>
            <div class="clear"></div>


        }
    </div>
    @Html.Action("Partial5", "Default")

</div>

